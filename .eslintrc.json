{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "import"],
  "overrides": [
    {
      "files": ["src/**/*.+(tsx|ts)"],
      "rules": {
        "@typescript-eslint/no-unused-vars": "error",
        "import/no-default-export": "error"
      }
    }
  ]
}
